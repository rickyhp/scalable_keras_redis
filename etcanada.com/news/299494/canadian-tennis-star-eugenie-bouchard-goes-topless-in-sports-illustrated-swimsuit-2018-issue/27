						iframe.contentWindow.postMessage( {
							'msg_type': 'poll_size',
							'frame_id': 'wpcom-iframe-7d2b4fe476e327943accba7e1ee2426d-5ab212edf014a'
						}, window.location.protocol + '//wpcomwidgets.com' );
